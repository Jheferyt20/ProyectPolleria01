<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Busquedaa">


    <Button
        android:id="@+id/pollos"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="52dp"
        android:background="@drawable/pollos"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/combos"
        tools:ignore="MissingConstraints" />



    <Button
        android:id="@+id/ensalada"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/ensaladas"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/pollos"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/promocion"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/parillas"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/parrillas"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:text=""
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/nav_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.202"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/postres"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="52dp"
        android:background="@drawable/postres"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:text=""
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/nav_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pollos"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/promocion8"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/bebidas"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:text=""
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/parillas"
        app:layout_constraintEnd_toStartOf="@+id/postres"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ensalada"
        app:layout_constraintVertical_bias="0.155"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/red_box"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/red"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/atras"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:paddingStart="10dp"
        android:src="@drawable/atras"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/promocion"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/promociones"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/nav_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/red_box"
        app:layout_constraintVertical_bias="0.008"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="16dp"
        android:text="PROMOCIONES"
        app:layout_constraintBottom_toTopOf="@+id/promocion"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/combos"
        android:layout_width="105dp"
        android:layout_height="91dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/combofamiliar"
        android:drawable="@drawable/curve_design"
        android:drawablePadding="8dp"
        android:gravity="center_vertical|center_horizontal"
        android:padding="16dp"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/nav_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.62"
        app:layout_constraintStart_toEndOf="@+id/promocion"
        app:layout_constraintTop_toBottomOf="@+id/red_box"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textCombos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="COMBOS"
        app:layout_constraintBottom_toTopOf="@+id/combos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.582"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="12dp"
        android:text="ENSALADAS"
        app:layout_constraintBottom_toTopOf="@+id/ensalada"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="POLLOS"
        app:layout_constraintBottom_toTopOf="@+id/pollos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textBebida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="BEBIDAS"
        app:layout_constraintBottom_toTopOf="@+id/promocion8"
        app:layout_constraintEnd_toStartOf="@+id/textPostres"
        app:layout_constraintHorizontal_bias="0.376"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textPostres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="8dp"
        android:text="POSTRES"
        app:layout_constraintBottom_toTopOf="@+id/postres"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textParilla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="PARILLAS"
        app:layout_constraintBottom_toTopOf="@+id/parillas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/texto_blanco"
        android:layout_width="90dp"
        android:layout_height="34dp"
        android:gravity="center"
        android:text="MENUS"
        android:textColor="@android:color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/red_box"
        app:layout_constraintEnd_toEndOf="@id/red_box"
        app:layout_constraintStart_toStartOf="@id/red_box"
        app:layout_constraintTop_toTopOf="@id/red_box" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:background="@color/app"
        style="@style/BottomNavigationStyle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/bottom_nav_menu" />


</androidx.constraintlayout.widget.ConstraintLayout>